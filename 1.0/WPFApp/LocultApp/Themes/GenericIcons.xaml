<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    
    xmlns:view="clr-namespace:MsgBox.View"
    >
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/FsCore;component/Images/Metro/GenericIcons.xaml" />
    </ResourceDictionary.MergedDictionaries>

  <!-- These icons can be displayed in a message display. They are setup to work for non-styled (generic) UI.
       Just include this file in your application's ResourceDictionary and the ImageEnumToImageConverter
       shall pick it up from there.
   -->
    <BitmapImage x:Key="AppMessageError_Image" UriSource="/LocultApp;component/Images/Message/Generic/48px-Dialog-error.svg.png" />
    
    <BitmapImage x:Key="Locult_Image" UriSource="/LocultApp;component/Icons/appbar.snowflake_48.png" />

</ResourceDictionary>
