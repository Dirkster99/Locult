<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    
    xmlns:view="clr-namespace:MsgBox.View"
    >
    <!-- These icons can be displayed in a message display. They are setup to work for dark Metro styled GUI.
       Just include this file in your application's ResourceDictionary and the ImageEnumToImageConverter
       shall pick it up from there.
   -->
    <BitmapImage x:Key="AppMessageError_Image" UriSource="/LocultApp;component/Images/Message/Metro/Dark/appbar.sign.stop.png" />

</ResourceDictionary>
